IOL = {}

loadScript = (url, callback) ->
	js = document.createElement "script"
	js.type = "text/javascript"
	js.src = url
	js.onload = callback
	js.onreadystatechange = ()->
		callback() if this.readyState == "complete"
	document.getElementsByTagName('head')[0].appendChild(js);


